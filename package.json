{
  "name": "orion",
  "version": "1.0.0",
  "description": "Chatbot for NM",
  "main": "ClientServer/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/Dash2Labs/Orion.git"
  },
  "engines": {
    "node": ">=21.5.0"
  },
  "scripts": {
    "build:js::dev": "webpack --config webpack.dev.js --progress --color --no-cache",
    "build:js": "webpack --config webpack.prod.js",
    "build:prod": "tsc --project ./tsconfig.json && export NODE_ENV='production' && npm run build:js",
    "build": "tsc --project ~/Orion/tsconfig.json && tsc && export NODE_ENV='development' && npm run build:js::dev",
    "watch": "tsc && npm run build:js::dev -- --watch",
    "server:dev": "nodemon ClientServer/index.js --watch --dev",
    "server": "node ClientServer/index.js",
    "dev": "npm run build && npm run server:dev",
    "start": "npm run server"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/material-ui": "^0.21.16",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.11.0",
    "esbuild-loader": "^4.0.2",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "nodemon": "^3.0.2",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "author": "Dustin Morris <dustin.morris@dash2labs.com>",
  "dependencies": {
    "@azure/msal-browser": "^3.10.0",
    "@azure/msal-react": "^2.0.12",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/base": "^5.0.0-beta.34",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "axios": "1.6.5",
    "babel-loader": "^9.1.3",
    "compression": "^1.7.4",
    "core-js": "^3.35.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "stream": "^0.0.3",
    "webpack-dev-server": "^5.0.4",
    "xss": "^1.0.14"
  }
}
