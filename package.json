{
  "name": "dustinwmorris",
  "version": "1.0.0",
  "description": "Chatbot UI",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:clienttypes && npm run build:server && npm run build:js",
    "build:server": "tsc -p ./clientserver/tsconfig.server.json",
    "build:clienttypes": "tsc -p src/tsconfig.client.json",
    "build:js": "webpack --config webpack.dev.js",
    "encrypt": "./node_modules/.bin/dotenvx encrypt -f clientserver/.env.local && ./node_modules/.bin/dotenvx encrypt -f clientserver/.env.production && ./node_modules/.bin/dotenvx encrypt -f .env",
    "start": "npm run build && npm run encrypt && dotenvx run -f clientserver/.env.local -- node ./clientserver/index.js"
  },
  "author": "Dustin Morris",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@azure/msal-browser": "^3.26.1",
    "@dotenvx/dotenvx": "^1.7.0",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "latest",
    "@mui/base": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-static-gzip": "^2.1.7",
    "morgan": "^1.10.0",
    "react": "latest",
    "react-dom": "latest",
    "uuid": "^10.0.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@eslint/js": "^9.13.0",
    "@testing-library/react": "^16.0.1",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/material-ui": "latest",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.2.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/supertest": "^6.0.2",
    "@types/testing-library__react": "^10.2.0",
    "babel-jest": "^29.7.0",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "filemanager-webpack-plugin": "^8.0.0",
    "globals": "^15.11.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "mini-css-extract-plugin": "^2.9.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.11.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
